{% for interface in interfaces %}
interface {{interface["name"]}}
    ip address {{ interface["ip"] }} {{ interface["mask"] }}
    {% if interface["shutdown"] %}
        shutdown
    {% else %}
        no shutdown
    {% endif %}
{% endfor %}

router ospf {{ ospf["process"] }}
    network {{ ospf["network"] }} {{ospf["wildcard"]}} area {{ ospf["area"]}}


    